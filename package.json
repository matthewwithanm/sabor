    "build": "babel src -d lib",
    "lint": "eslint",
    "lint-all": "eslint ."
  },
  "dependencies": {
    "babel-polyfill": "6.7.2",
    "es6-promise": "3.0.2",
    "babel-types": "6.7.2",
    "babylon": "6.7.0",
    "resolve": "1.1.6"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.6.0",
    "babel-eslint": "6.0.2",
    "babel-plugin-syntax-flow": "6.5.0",
    "babel-plugin-transform-flow-strip-types": "6.7.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "1.10.3",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-jasmine": "1.6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  },
  "bugs": {
    "url": "https://github.com/peterhal/sabor/issues"
  },
  "homepage": "https://github.com/peterhal/sabor#readme",
