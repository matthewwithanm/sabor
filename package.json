{
  "name": "sabor",
  "version": "0.7.0",
  "description": "Detects architecture cycles in JS directories.",
  "license": "MIT",
  "bin": {
    "sabor": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterhal/sabor.git"
  },
  "scripts": {
